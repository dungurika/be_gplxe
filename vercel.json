{
  "version": 2,
  "env": {
    "MONGODB_URL": "mongodb://localhost:27017/project_gplx"
  },
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "projectSettings": {
    "framework": "nestjs"
  }
}
